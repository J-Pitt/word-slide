# Production Environment Variables for AWS
# Copy this to your AWS Amplify environment variables

# API Configuration
REACT_APP_API_BASE=https://your-api-gateway-url.amazonaws.com/dev/api

# Game Configuration
REACT_APP_GAME_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production

# Analytics (optional)
REACT_APP_ANALYTICS_ID=your-analytics-id

# Feature Flags
REACT_APP_ENABLE_LEADERBOARDS=true
REACT_APP_ENABLE_USER_STATS=true
REACT_APP_ENABLE_SOCIAL_FEATURES=false
